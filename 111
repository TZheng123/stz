def number():

    str_num=input("input:")
    count1=0
    count2=0
    count3=0
    count4=0
    for i in str_num:
        if i.isdigit():
            count1+= 1
        elif i.isalpha():
            count2+=1

        elif i.isspace():
            count3 += 1
        else:

            count4+=1

    print("the num is ",count1)
    print("the zimu is ",count2)

    print("the kongge is ",count3)

    print("the qita is ",count4)

    number()

number()







def number():

    a=input("input:")
    a.replace(" ","")
    print(a)


number()






import random
i = 1
a = random.randint(0,100)
b = int(input('input:'))
while a != b:
    if a > b:
        print('so small')
        b = int(input('input:'))
    else:
        print('so big')
        b = int(input('input:'))
    i+=1
else:
    print(i,b)
